#pragma once

#include <QWidget>
struct QAction;
struct QMenu : QWidget {
  QMenu(QWidget* parent = nullptr) :
      QWidget(parent) {}
  QMenu(char const*, QWidget* parent = nullptr) :
      QWidget(parent) {}

  auto addSeparator() {}
  auto addMenu(QMenu*) noexcept {}
  auto addAction(QAction*) noexcept {}
};
