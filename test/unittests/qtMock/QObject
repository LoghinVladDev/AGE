#pragma once

struct QObject {
  explicit QObject(QObject* = nullptr) {}

  template <
      typename PointerToMemberFunction,
      typename Functor
  > static auto connect(QObject*, PointerToMemberFunction, Functor) { return; }

  template <
      typename PointerToMemberFunction,
      typename Functor
  > static auto connect(QObject*, PointerToMemberFunction, QObject*, Functor) { return; }
};
