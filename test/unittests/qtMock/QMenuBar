#pragma once

#include <QWidget>
#include <QMenu>
#include <QAction>
struct QMenuBar : QWidget {
  QMenuBar(QWidget* parent = nullptr) :
      QWidget(parent) {}

  auto addSeparator(){}
  auto addMenu(QMenu*) noexcept {}
  auto addAction(QAction*) noexcept {}
};
